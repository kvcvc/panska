{
    "private": true,
    "name": "studentsky-portal",
    "engines": {
        "node": "14.15.4"
    },
    "scripts": {
        "client": "yarn workspace client start",
        "server": "yarn workspace server start",
        "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\""
    },
    "workspaces": ["packages/server", "packages/client"],
    "dependencies": {
        "concurrently": "5.3.0"
    }
}